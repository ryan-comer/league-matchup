{
  "name": "league-matchup-utils",
  "author": {
    "name": "Ryan Comer",
    "email": "ryancomer94@gmail.com"
  },
  "description": "League Matchup Utils is a utility app for League of Legends.",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryan-comer/league-matchup.git"
  },
  "version": "1.1.1",
  "private": true,
  "main": "main.js",
  "homepage": "./",
  "build": {
    "appId": "league.matchup.utils",
    "productName": "League Matchup Utils",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "main.js",
      "preload.js"
    ],
    "extends": null
  },
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
    "@mui/material": "^5.8.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "electron-is-dev": "^2.0.0",
    "electron-updater": "^5.0.1",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "dist": "electron-builder -p always",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev": "concurrently -k \"yarn start\" \"yarn electron .\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^7.2.0",
    "electron": "^18.2.4",
    "electron-builder": "^23.0.3",
    "wait-on": "^6.0.1"
  }
}
